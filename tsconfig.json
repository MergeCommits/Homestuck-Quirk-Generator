{
    "extends": "@tsconfig/create-react-app/tsconfig.json",
    "compilerOptions": {
        "baseUrl": "./src",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}
